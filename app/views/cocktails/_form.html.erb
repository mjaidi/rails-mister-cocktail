<%= simple_form_for(cocktail) do |f| %>
  <div class = "form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class = "form-group">
    <%= f.label :image %>
    <%= f.text_field :image, class: 'form-control' %>
  </div>
  <%= f.submit class: "btn btn-primary"%>
<% end %>